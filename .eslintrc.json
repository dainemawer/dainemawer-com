{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "parser": "babel-eslint",
    "globals": {
        "React": "writable",
        "require": true,
        "process": true,
        "exports": true,
        "module": true,
        "__dirname": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/display-name": "off",
        "sort-keys": ["error", "asc", {"caseSensitive": true, "natural": false, "minKeys": 2}]
    }
}